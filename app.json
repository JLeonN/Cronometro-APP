{
  "expo": {
    "name": "Cron√≥metro",
    "slug": "Nombre-de-mi-proyecto",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/iconNuevo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~1458002511"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconNuevo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yojorge.Nombredemiproyecto",
      "permissions": ["com.google.android.gms.permission.AD_ID"],
      "config": {
        "googleMobileAds": {
          "androidAppId": "ca-app-pub-7620083100302566~2103393503"
        }
      },
      "targetSdkVersion": 35
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/faviconNuevo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-iconNuevo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "react-native-google-mobile-ads"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "511bc9ee-ef5c-4969-a4fb-9bbf5e2b28bd"
      }
    },
    "owner": "yojorge"
  }
}
